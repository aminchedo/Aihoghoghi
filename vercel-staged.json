{
  "version": 2,
  "builds": [
    {
      "src": "main-vercel-staged.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/main-vercel-staged.py"
    },
    {
      "src": "/(.*)",
      "dest": "/main-vercel-staged.py"
    }
  ],
  "functions": {
    "main-vercel-staged.py": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "regions": ["iad1"],
  "env": {
    "PYTHONPATH": "/var/task",
    "NODE_ENV": "production",
    "TRANSFORMERS_CACHE": "/tmp/transformers_cache"
  }
}