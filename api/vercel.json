{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/main.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/main.py"
    }
  ],
  "functions": {
    "api/main.py": {
      "runtime": "python3.12",
      "maxDuration": 60
    }
  }
}