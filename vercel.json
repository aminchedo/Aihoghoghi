{
  "functions": {
    "api/main.py": {
      "runtime": "@vercel/python",
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/main.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/main.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11",
    "PYTHONPATH": "/var/task"
  },
  "regions": ["iad1"]
}