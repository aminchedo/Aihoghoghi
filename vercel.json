{
  "functions": {
    "api/main.py": {
      "runtime": "@vercel/python",
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/main.py"
    },
    {
      "src": "/health",
      "dest": "/api/main.py"
    },
    {
      "src": "/home",
      "dest": "/api/main.py"
    },
    {
      "src": "/pages",
      "dest": "/api/main.py"
    },
    {
      "src": "/",
      "dest": "/api/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/main.py"
    }
  ],
  "regions": ["iad1"],
  "env": {
    "PYTHONPATH": "/var/task",
    "PYTHON_VERSION": "3.11"
  }
}